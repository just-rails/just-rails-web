<p id="notice"><%= notice %></p>

<h1>Listing Gem Reviews</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gem_reviews.each do |gem_review| %>
      <tr>
        <td><%= link_to 'Show', gem_review %></td>
        <td><%= link_to 'Edit', edit_gem_review_path(gem_review) %></td>
        <td><%= link_to 'Destroy', gem_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gem review', new_gem_review_path %>
